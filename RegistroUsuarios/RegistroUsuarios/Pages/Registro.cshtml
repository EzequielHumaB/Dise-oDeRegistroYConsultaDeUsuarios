@page
@model RegistroUsuarios.Pages.RegistroModel
@{
}

<title>Registro de ususarios</title>
<h4>Usuarios</h4>
<div class="row">
    <div class="col-md-4 ">
        <form method="post">
            <div class="form-group">
                <label>Nombre</label>
                <input type="text" id="Nombre" class="form-check"  /> 
            </div>
            <div class="form-group">
                <label>Descripcion </label>
                <input type="text" id="NombreTextBox" class="form-check" />      
            </div>       

            <div class="form-group">
                <label>Fecha</label>
                <input type="date" id="FechaDateTime" class="form-check"/>
            </div>

            <div class="form-group">
                <input type="submit" value="Crear usuario" id="BotonCrear" class="btn btn-primary" />
            </div>

        </form>
    </div>
</div>